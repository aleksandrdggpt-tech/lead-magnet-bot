# Telegram Bot Token
# Получите у @BotFather в Telegram
BOT_TOKEN=your_bot_token_here

# Admin User IDs (через запятую)
# Узнайте свой Telegram ID у @userinfobot
ADMIN_USER_IDS=123456789,987654321

# Channel Username для проверки подписки (опционально)
# Можно настроить через команду /set_channel в админ-панели
# Если не указано, будет использовано значение по умолчанию
CHANNEL_USERNAME=@TaktikaKutuzova

# Database URL
# Для локальной разработки с SQLite (DEV_MODE=1) можно не указывать
# Для Railway будет установлен автоматически
DATABASE_URL=postgresql://user:password@host:port/database

# Development Mode (использовать SQLite локально)
# Установите в 1 для локальной разработки без PostgreSQL
DEV_MODE=0

# Database Pool Settings (опционально)
DB_POOL_SIZE=5
DB_MAX_OVERFLOW=0
